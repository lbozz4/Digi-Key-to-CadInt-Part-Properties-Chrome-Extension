{
    "name": "Digi-CadInt",
    "description": "Scrapes part info off Digi-Key and creates an XML file that CadInt can import into a new part.",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon_disabled.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "downloads"
    ],
    "background" : {
        "scripts" : ["background.js"]
      },
    "content_scripts": [
        {
            "matches": [
                "https://www.digikey.com/product-detail/*"
            ],
            "js": [
                "jquery-3.4.1.min.js"
            ]
        }
    ]
}